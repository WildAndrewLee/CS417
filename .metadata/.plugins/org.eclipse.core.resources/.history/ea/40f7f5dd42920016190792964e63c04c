import java.rmi.Naming;
import java.rmi.RemoteExeption;

public class Client {
	public static void main(String[] args){
		try{
			String host = "localhost";
			int port = 1099;
			
			for(String arg : args){
				if(arg.startsWith("-h")){
					host = arg.substring(3);
				}
			}
		}
		catch(Exception e){
			System.err.println("Unable to start client.");
			e.printStackTrace();
		}
	}
}